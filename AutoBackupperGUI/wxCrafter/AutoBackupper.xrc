                            <label>Recur every</label>
                            <style/>
                            <hidden>1</hidden>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <flag>wxALL|wxRESERVE_SPACE_EVEN_IF_HIDDEN</flag>
                          <border>5</border>
                          <object class="wxSpinCtrl" name="spinCtrlRecurrence">
                            <style/>
                            <hidden>1</hidden>
                            <value>1</value>
                            <min>1</min>
                            <max>999</max>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <flag>wxALIGN_CENTER_VERTICAL|wxRESERVE_SPACE_EVEN_IF_HIDDEN</flag>
                          <border>5</border>
