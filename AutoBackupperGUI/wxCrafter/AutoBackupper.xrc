                  </object>
                </object>
              </object>
              <object class="sizeritem">
                <flag>wxLEFT|wxRIGHT|wxTOP|wxEXPAND</flag>
                <border>5</border>
                <option>1</option>
                <object class="wxPanel" name="panelRecurrenceDays">
                  <style>wxTAB_TRAVERSAL|wxBORDER_THEME</style>
                  <object class="wxBoxSizer">
                    <orient>wxHORIZONTAL</orient>
                    <object class="sizeritem" name="sizerRecurrence">
